{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "1.0.0",
  "default_locale": "en",
  "permissions": [
    "storage",
    "sidePanel",
    "tabs",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "https://www.notion.so/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "__MSG_openSidePanel__"
  },
  "side_panel": {
    "default_path": "side_panel/side_panel.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}